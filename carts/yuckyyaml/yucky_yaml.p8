-- shared

function anim_item(array,speed)
	local i=flr(_t/speed%#array)+1
	return array[i]
end

function is_in_sequence(val,sequence)
	for item in all(sequence) do
		if item==val then
			return true
		end
	end
	return false
end

function get_index(val,sequence)
	local i=1
	for item in all(sequence) do
		if item==val then
			return i
		end
		i+=1
	end
	return nil
end
