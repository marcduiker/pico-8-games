function update_row_col()
	if row_new<rows[1] then row_new=rows[3]
	elseif row_new>rows[3] then row_new=rows[1] 
	end	
	
	if col_new<cols[1] then col_new=cols[9]
	elseif col_new>cols[9] then col_new=cols[1]
	end
	
	row_select.y=row_new
	col_select.x=col_new
end

function highlight()
	local card_act=mget(col_new,row_new)
	if card_h.prev>0 then
		mset(card_h.col,card_h.row,card_h.prev)
	end	
	
	local i=get_index(card_act,cards_spr_toplay)
	if i==nil then
		i=get_index(card_act,cards_spr_played)
	end
	
	if i!=nil then
	 card_new=cards_spr_high[i]
		mset(col_new,row_new,card_new)
		card_h.col=col_new
		card_h.row=row_new
		card_h.hi=card_new
		card_h.prev=card_act
	end	
end

function change_card()
	card_h.hi+=1
	if card_h.hi>cards_spr_high[5] then
		card_h.hi=cards_spr_high[1]
	elseif card_h.hi<cards_spr_high[1] then
	 card_h.hi=cards_spr_high[5]
	end
	local i=get_index(card_h.hi,cards_spr_high)
	card_h.prev=cards_spr_toplay[i]
	mset(card_h.col,card_h.row,card_h.hi)
end

