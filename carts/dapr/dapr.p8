function _init()
	init_game()
end

function init_game()
	music(0,1000)
	init_map()
	add_player()
	dirx={-1,1,0,0}
	diry={0,0,-1,1}
	t=0
	ti=0
	game_over=false
	btnbuffer=-1
	_upd=update_start_menu
	_drw=draw_start_menu
end

function _update()
	t+=1
	_upd()
end

function _draw()
	cls()
	draw_map()
	_drw()
end

