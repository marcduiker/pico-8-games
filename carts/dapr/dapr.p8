function update_game_over()
	music(-1,1000)
	if btnp(âŽ) then
		poke(0x5f80, getactivelink())
	elseif btnp(ðŸ…¾ï¸) then
		run()
	elseif btnp(3) then
		-- down
		sfx(0)
		currentindex=getactivelink()
		if currentindex == #linkmenu then
			nextindex=1
		else
			nextindex+=1
		end
	elseif btnp(2) then
		-- up
		sfx(0)
		currentindex=getactivelink()
		if currentindex == 1 then
