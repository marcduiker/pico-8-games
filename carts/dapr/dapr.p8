	music(0,1000,11)
	init_map()
	add_player()
	dirx={-1,1,0,0}
	diry={0,0,-1,1}
	t=0
	
	game_over=false
	btnbuffer=-1
	_lvl=levels[1]
	_msg=message0
	_upd=update_menu
	_drw=draw_menu
end

function _update()
	t+=1
	_upd()
end

function _draw()
	cls()
	if p.isplaying then
		draw_map()
	end
	_drw()
end

