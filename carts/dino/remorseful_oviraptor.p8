function _init()
	init_game()
end

function init_game()
	init_dinos()
	init_water()
	add_player()
	dirx={-1,1,0,0}
	diry={0,0,-1,1}
	t=0
	start_time=nil
	elapsed_time=nil
	game_over=false
	target=4
	score=0
	btnbuffer=-1
	_upd=update_menu
	_drw=draw_menu
end


function _update()
	t+=1
	_upd()
end

function _draw()
	cls()
	draw_dinos()
	draw_water()
	draw_time()
	_drw()
end
