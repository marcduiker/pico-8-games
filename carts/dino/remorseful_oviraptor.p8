function init_water()
	water={
		anim_speed=15,
		anim_move={38,39}
	}
end

function draw_water()
	for x=0,15 do
		for y=0,15 do
			local _tile=mget(x,y)
			if fget(_tile,3) then
				mset(x,y,get_frame(water.anim_move,water.anim_speed))
			end
		end
	end
end
