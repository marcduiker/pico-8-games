function update_message_section()
	if btnp(❎) then
		if p.isgameover then
			_upd=update_game_over
			_drw=draw_game_over
		else
			sfx(2)
			_upd=update_player_move
			_drw=draw_player
		end
	end
end

function draw_message_section()
	local x1=0
	local x2=128
	rectfill(x1,40,x2,104,1)
	rect(x1+2,42,x2-2,102,7)
	print_message(p.message.line1, 50, 7)
	print_message(p.message.line2, 60, 7)
	print_message(p.message.line3, 70, 7)
	print_message(p.message.line4, 80, 7)
	print_message(p.message.action, 90, 10)
end

function print_message(text, x, color)
	if #text > 0 then
		print(text, hcenter(text), x, color)
	end
end

function update_game_over()
	if btnp(🅾️) then
		run()
	end
end

function draw_game_over()
	local line1="thanks for playing!"
	local line2="you can find me at"
	local line3="twitter.com/marcduiker"
	local line4="mstdn.social/@marcduiker"
	local line5="github.com/marcduiker"
	local action="press 🅾️/c to play again"
	local x1=0
	local x2=128
	rectfill(x1,28,x2,106,1)
	rect(x1+2,30,x2-2,104,7)
	print_message(line1,38,10)
	print_message(line2,50,7)
	print_message(line3,60,7)
	print_message(line4,70,7)
	print_message(line5,80,7)
	print_message(action,94,10)
end

function hcenter(s)
  return 64-#s*2
end
