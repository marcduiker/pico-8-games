pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
local sprites={33,34,35,36}
local sprites_wind={1,3,6,7,8,7,6,3,1}
local spr_id=33
local _t
local speed=6

function _init()
	_t=0
end

function _update()
		--anim_spr()
		_t+=1
		if btnp(⬅️) then
			spr_id-=1
		elseif btnp(➡️) then
			spr_id+=1
		end
end


function anim_spr()
	local i=flr(_t/speed%#sprites)+1
	spr_id=sprites[i]
end

function _draw()
	cls()
	spr(spr_id,8*8,8*8,1,2)
end
