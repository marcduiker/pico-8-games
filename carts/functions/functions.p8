-- map

local map_tiles={
	server=17,
	floor=18
}

local map_tile_types={
	wall=0,
	collectable=1
}

function init_map()
	game_map={
		max_items=11,
		
		draw=function(self)
			map(0,0,0,0,16,16)
		end,
		
		seed_items=function(self)
			--local max_iterations=100
			local coords=create_item_coordinates()
			local items_created=0
			for c in all(coords) do
				local spr_id=flr(rnd(6))+4
				if not is_wall(c.x,c.y) and items_created<self.max_items then
				 mset(c.x,c.y,spr_id)
				 items_created+=1
				end
			end
		end
	}
end

function create_item_coordinates()
	local coordinates={}
	for i=1,100 do
		local x_val=flr(rnd(14))+1
		local y_val=flr(rnd(14))+1
		local xy={x=x_val,y=y_val}
		local is_found=false
		if i==1 then 
			add(coordinates,xy)
		else
			for c in all(coordinates) do
			 if c.x==xy.x and c.y==xy.y then
				 is_found=true
				 break
			 else
				 add(coordinates,xy)
				 break
			 end	
		 end
		end
	end
	return coordinates
end

function is_wall(x,y)
	local tile=mget(x,y)
	return fget(tile,map_tile_types.wall)
end

function is_collectable(x,y)
	local tile=mget(x,y)
	return fget(tile,map_tile_types.collectable)
end

function remove_item(x,y)
	mset(x,y,map_tiles.floor)
end

function can_move(x,y)
	return not is_wall(x,y)
end
