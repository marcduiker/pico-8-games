-- player
local sprites={
	normal=1,
	special=2
}

local sounds={
	move=0,
	special_on=1,
	special_off=2,
	bump=3,
	pickup=4
}

function init_player()
	local step = 1
	player = {
 	x=8,
		y=8,
	 normal=true,
	 score=0,
	 moves=0,
	  
		update = function(self)
	 	local new_x=self.x
	 	local new_y=self.y
	 	local old_x=self.x
	 	local old_y=self.y
	 	
	 	if (btnp(‚¨ÖÔ∏è)) then 
	 		new_x-=step
	 		self.moves+=1 
	 	end
	 	if (btnp(‚û°Ô∏è)) then 
	 		new_x+=step
	 		self.moves+=1 
	 	end
	 	if (btnp(‚¨ÜÔ∏è)) then 
	 		new_y-=step
	 		self.moves+=1 
	 	end
	 	if (btnp(‚¨áÔ∏è)) then 
	 		new_y+=step
	 		self.moves+=1 
	 	end
		 
		 if can_move(new_x, new_y) then
		 	self.x=new_x
		 	self.y=new_y
		 	if old_x!=self.x or old_y!=self.y then
		 		self:play_move_sound()
		 	end
		 else
		 	self:play_bump_sound()
		 end
		 
		 if is_collectable(self.x, self.y) then
		 	remove_item(self.x, self.y)
		 	self:play_pickup_sound()
		 	self.score+=1
		 end
		 
		 if self.score==game_map.max_items then
		 	game.state=states.end_game
		 end
		 
		 if (btnp(‚ùé)) then 
		 	if (not self.normal) then
		 		self.normal = true
		 	else
		 		self.normal = false 
		 	end
		 	self:play_special_sound()
			end
		end,
	 
		draw=function(self)
		 --sprite
		 local spr_id
		 if (self.normal) then
				spr_id=sprites.normal
			else
				spr_id=sprites.special
			end
			spr(spr_id,self.x*8,self.y*8)
			
			--score
			print("score",0,0,12)
 		print(self.score,25,0,12)
 		--moves
 		print("moves",90,0,12)
 		print(self.moves,116,0,12)
 		
		end,
		
		play_move_sound = function(self)
				sfx(sounds.move,0,0)
		end,
		
		play_bump_sound = function(self)
				sfx(sounds.bump,0,0)
		end,
		
		play_pickup_sound = function(self)
				sfx(sounds.pickup,0,0)
		end,
		
		play_special_sound = function(self)
			local sfx_id
			if (self.normal) then
				sfx_id = sounds.special_off
			else
				sfx_id = sounds.special_on
			end
				sfx(sfx_id,0,0)
		end,
	}
end
